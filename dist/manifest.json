{
  "name": "Akashi Master",
  "description" : "akashi",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "js/background_script.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "https://atnd.ak4.jp/*"
      ],
      "js": [
        "js/content_script.js"
      ],
      "run_at": "document_end"
    }
  ]
}